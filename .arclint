{
  "debug" : true,
  "linters" : {
    "pep8" : {
      "type" : "pep8",
      "include" : "(\\.py$)",
      "exclude" : "(^exclude.py)",
      "severity" : {
        "E401" : "warning"
      }
    },
    "flake8" : {
      "type" : "flake8",
      "include" : "(\\.py$)",
      "exclude" : "(^exclude\\.py)",
      "severity" : {
        "E901" : "advice"
      }
    },
    "csslint" : {
      "type" : "csslint",
      "include" : "(\\.css$)"
    }
  }
}
